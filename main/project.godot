; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Object",
"class": &"SceneManagerConstants",
"language": &"GDScript",
"path": "res://addons/scene_manager/SceneManagerConstants.gd"
}]
_global_script_class_icons={
"SceneManagerConstants": ""
}

[application]

config/name="Neo Tutui"
run/main_scene="res://Boot.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/show_image=false
boot_splash/bg_color=Color(0.0941176, 0, 0.196078, 1)
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://core/default_bus_layout.tres"

[autoload]

ControlesFixos="*res://core/ControlesFixos.gd"
Anima="*res://ui/Anima.gd"
Som="*res://core/Som.tscn"
Mouse="*res://ui/cursor/Mouse.tscn"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Change="*res://core/ChangeScene.gd"
Data="*res://core/Data.tscn"

[compression]

formats/zstd/compression_level=4

[dialogic]

editor/character_preview_mode=0
editor/current_editor=&"Settings"
editor/current_resources={
"Timeline Editor": "res://cenas/Teste/Dialogos/Teste.dtl"
}
save/autosave_delay=60.0
glossary/default_case_sensitive=true
choices/autofocus_first=true
choices/delay=0.2
text/speed=0.01
text/skippable=true
text/autocontinue_delay=1.0
history/history_system=true
history/full_history=true
history/text_history=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
variables={}
editor/current_timeline_path="res://cenas/Teste/Dialogos/Teste.dtl"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

movie_writer/mix_rate=44000
movie_writer/mjpeg_quality=0.85
node_naming/name_num_separator=2
scene/scene_naming=1

[editor_plugins]

enabled=PackedStringArray("res://addons/scene_manager/plugin.cfg")

[input]

panic={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194326,"unicode":0,"echo":false,"script":null)
]
}

[physics]

common/max_physics_steps_per_frame=4
3d/solver/solver_iterations=4

[rendering]

occlusion_culling/bvh_build_quality=1
renderer/rendering_method="gl_compatibility"
textures/webp_compression/compression_method=3
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=0
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=0
gl_compatibility/item_buffer_size=8000
reflections/sky_reflections/roughness_layers=6
reflections/sky_reflections/ggx_samples=16
reflections/reflection_atlas/reflection_size=128
reflections/reflection_atlas/reflection_count=32
textures/default_filters/use_nearest_mipmap_filter=true
textures/default_filters/anisotropic_filtering_level=0
environment/ssao/quality=0
environment/ssil/quality=0
anti_aliasing/screen_space_roughness_limiter/enabled=false
environment/glow/upscale_mode=0
limits/opengl/max_renderable_elements=32000
limits/opengl/max_renderable_lights=16
limits/opengl/max_lights_per_object=4
